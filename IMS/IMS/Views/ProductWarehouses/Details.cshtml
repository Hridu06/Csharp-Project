@model IMS.Models.ProductWarehouse

<h2>Product-Warehouse Details</h2>
<div>
    <dl class="row">
        <dt class="col-sm-2">Product</dt>
        <dd class="col-sm-10">@Model.Product.Name</dd>

        <dt class="col-sm-2">Warehouse</dt>
        <dd class="col-sm-10">@Model.Warehouse.Name</dd>

        <dt class="col-sm-2">Quantity</dt>
        <dd class="col-sm-10">@Model.Quantity</dd>

        <dt class="col-sm-2">Last Updated</dt>
        <dd class="col-sm-10">@Model.LastUpdated</dd>
    </dl>
</div>
<a asp-action="Edit" asp-route-id="@Model.ProductWarehouseId" class="btn btn-warning">Edit</a>
<a asp-action="Index" class="btn btn-secondary">Back</a>
